;
; K_CMOS_PortIO.inf
;

[Version]
Signature="$WINDOWS NT$"
Class=Sample ; TODO: edit Class
ClassGuid={78A1C341-4539-11d3-B88D-00C04FAD5171} ; TODO: edit ClassGuid
Provider=%ManufacturerName%
CatalogFile=K_CMOS_PortIO.cat
DriverVer= ; TODO: set DriverVer in stampinf property pages
PnpLockDown=1

[DestinationDirs]
DefaultDestDir = 12
K_CMOS_PortIO_Device_CoInstaller_CopyFiles = 11

; ================= Class section =====================

[ClassInstall32]
Addreg=SampleClassReg

[SampleClassReg]
HKR,,,0,%ClassName%
HKR,,Icon,,-5

[SourceDisksNames]
1 = %DiskName%,,,""

[SourceDisksFiles]
K_CMOS_PortIO.sys  = 1,,
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll=1 ; make sure the number matches with SourceDisksNames

;*****************************************
; Install Section
;*****************************************

[Manufacturer]
%ManufacturerName%=Standard,NT$ARCH$

[Standard.NT$ARCH$]
%K_CMOS_PortIO.DeviceDesc%=K_CMOS_PortIO_Device, Root\K_CMOS_PortIO ; TODO: edit hw-id

[K_CMOS_PortIO_Device.NT]
CopyFiles=Drivers_Dir

[Drivers_Dir]
K_CMOS_PortIO.sys

;-------------- Service installation
[K_CMOS_PortIO_Device.NT.Services]
AddService = K_CMOS_PortIO,%SPSVCINST_ASSOCSERVICE%, K_CMOS_PortIO_Service_Inst

; -------------- K_CMOS_PortIO driver install sections
[K_CMOS_PortIO_Service_Inst]
DisplayName    = %K_CMOS_PortIO.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\K_CMOS_PortIO.sys

;
;--- K_CMOS_PortIO_Device Coinstaller installation ------
;

[K_CMOS_PortIO_Device.NT.CoInstallers]
AddReg=K_CMOS_PortIO_Device_CoInstaller_AddReg
CopyFiles=K_CMOS_PortIO_Device_CoInstaller_CopyFiles

[K_CMOS_PortIO_Device_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll,WdfCoInstaller"

[K_CMOS_PortIO_Device_CoInstaller_CopyFiles]
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll

[K_CMOS_PortIO_Device.NT.Wdf]
KmdfService =  K_CMOS_PortIO, K_CMOS_PortIO_wdfsect
[K_CMOS_PortIO_wdfsect]
KmdfLibraryVersion = $KMDFVERSION$

[Strings]
SPSVCINST_ASSOCSERVICE= 0x00000002
ManufacturerName="<Your manufacturer name>" ;TODO: Replace with your manufacturer name
ClassName="Samples" ; TODO: edit ClassName
DiskName = "K_CMOS_PortIO Installation Disk"
K_CMOS_PortIO.DeviceDesc = "K_CMOS_PortIO Device"
K_CMOS_PortIO.SVCDESC = "K_CMOS_PortIO Service"
